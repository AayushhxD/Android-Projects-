<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="30dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp"
            android:layout_marginRight="1dp">

            <TextView
                android:id="@+id/textView"
                android:layout_width="150dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Roll Number"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/rollnot1"
                android:layout_width="240dp"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="150dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Name"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/namet2"
                android:layout_width="240dp"
                android:layout_height="wrap_content" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp">

            <TextView
                android:id="@+id/textViewsem"
                android:layout_width="150dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Semester"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/semestert6"
                android:layout_width="240dp"
                android:layout_height="wrap_content" />
        </TableRow>
    </TableLayout>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Marks"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Marks Obtained"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Maximum Marks"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp" >

            <TextView
                android:id="@+id/textView11"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Insem Marks"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/classtestt4"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="150" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp" >

            <TextView
                android:id="@+id/textView14"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Practical Marks"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/sessionalt5"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="100" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp" >

            <TextView
                android:id="@+id/textView17"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Assignment Marks"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/assignmentt7"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="100" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/textView20"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content"
                android:text="Total Marks"
                android:background="@color/colorPrimary"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/Totalt8"
                android:layout_width="130dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="350" />
        </TableRow>
    </TableLayout>


</LinearLayout>